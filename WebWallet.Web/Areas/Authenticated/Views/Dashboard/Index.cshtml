@model IEnumerable<MonthlyReportVM>
@{
    Layout = "_Layout";
    ViewData["title"] = "Начало";
}

@foreach (var report in Model)
{
    <h1>@report.Name</h1>
    <h1>@report.TotalIncome</h1>
    <h1>@report.TotalSpendings</h1>
    <h1>@report.EndBalance</h1>
}

<div class="row">
    <div class="col-lg-12 mt-5">
        <div class="card">
            <div class="card-body">
                <div id="incomeVsExpence" class="charts">
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <partial name="_DashboardCharts" />
}